{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "www",
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      ".gitkeep",
      "firebase.json",
      "service-worker.js"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
